{
  "$schema": "https://mintlify.com/docs.json",
  "name": "OnCall Debug",
  "theme": "mint",
  "colors": {
    "primary": "#2F8000",
    "light": "#45BD00",
    "dark": "#45BD00"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Home",
          "href": "https://oncall.mintlify.app/",
          "icon": "home"
        },
        {
          "anchor": "Main Website",
          "href": "https://oncall.build",
          "icon": "globe"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Blog",
        "groups": [
          {
            "group": "Categories",
            "pages": [
              "categories/architecture/index",
              "categories/code-intelligence/index",
              "categories/runtime-signals/index",
              "categories/llm-reasoning/index",
              "categories/debugging-cases/index",
              "categories/engineering-lessons/index",
              "categories/security-and-safety/index",
              "categories/future-of-debugging/index"
            ]
          },
          {
            "group": "Community",
            "pages": [
              "community/beyond-prompts",
              "community/discord-invite"
            ]
          }
        ]
      },
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "GETTING STARTED",
            "pages": [
              "docs/getting-started/overview",
              "docs/getting-started/quickstart",
              "docs/getting-started/common-workflows"
            ]
          },
          {
            "group": "BUILD WITH ONCALL",
            "pages": [
              "docs/build/architecture-overview",
              "docs/build/troubleshooting"
            ]
          },
          {
            "group": "ADMINISTRATION",
            "pages": [
              "docs/administration/advanced-installation",
              "docs/administration/iam",
              "docs/administration/security",
              "docs/administration/data-usage",
              "docs/administration/monitoring",
              "docs/administration/analytics"
            ]
          },
          {
            "group": "CONFIGURATION",
            "pages": [
              "docs/configuration/settings"
            ]
          },
          {
            "group": "REFERENCES",
            "pages": [
              "docs/references/commands"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact",
        "href": "https://github.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Early Access",
      "href": "https://github.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://github.com",
      "github": "https://github.com",
      "linkedin": "https://github.com"
    }
  }
}